{
  "name": "webship/vdo-project",
  "description": "Project template for VDO projects with composer",
  "type": "project",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "Webship",
      "homepage": "https://github.com/webship",
      "role": "Maintainer"
    }
  ],
  "support": {
    "issues": "http://drupal.org/project/issues/vdo",
    "source": "http://cgit.drupalcode.org/vdo"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ],
  "require": {
    "composer/installers": "^1.2",
    "cweagans/composer-patches": "^1.6",
    "drupal-composer/drupal-scaffold": "^2.2",
    "webship/vdo": "8.1.x-dev"
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "bin-dir": "bin/",
    "secure-http": false
  },
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-8.x-1.x": "8.1.x-dev"
    },
    "installer-paths": {
      "web/core": ["type:drupal-core"],
      "web/libraries/{$name}": ["type:drupal-library"],
      "web/modules/{$name}": ["type:drupal-module"],
      "web/profiles/{$name}": ["type:drupal-profile"],
      "web/themes/{$name}": ["type:drupal-theme"],
      "drush/{$name}": ["type:drupal-drush"]
    },
    "enable-patching": true
  }
}
